<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.contacts"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.contacts.ContactsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.contacts.AddContactActivity"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name="com.example.contacts.DataListView"
            android:label="@string/title_activity_data_list_view" >
        </activity>
        <activity
            android:name="com.example.contacts.ViewContact"
            android:label="@string/title_activity_view_contact" >
        </activity>
        <activity
            android:name="com.example.contacts.EditContact"
            android:label="@string/title_activity_edit_contact" >
        </activity>
    </application>

</manifest>
